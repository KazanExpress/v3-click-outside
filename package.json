{
  "name": "@kazanexpress/v3-click-outside",
  "version": "0.0.4",
  "description": "Vue 3 directive to react on clicks outside an element",
  "main": "dist/v-click-outside.umd.js",
  "umd:main": "dist/v-click-outside.umd.js",
  "types": "dist/types/index.d.ts",
  "source": "src/index.ts",
  "scripts": {
    "build": "microbundle --entry src/index.ts --target browser --format umd --name v3-click-outside",
    "lint": "eslint . --ext .js",
    "format": "prettier --write '**/*.{js,json,md}'",
    "format:changed": "pretty-quick",
    "format:staged": "pretty-quick --staged",
    "release": "np",
    "version": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KazanExpress/v3-click-outside.git"
  },
  "author": "ndelvalle <nicolas.delvalle@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KazanExpress/v3-click-outside"
  },
  "homepage": "https://github.com/KazanExpress/v3-click-outside#readme",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.11.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "microbundle": "^0.13.0",
    "np": "^7.2.0",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.0.0",
    "typescript": "3.8.3",
    "vue": "^3.0.0"
  },
  "engines": {
    "node": ">=6"
  }
}
